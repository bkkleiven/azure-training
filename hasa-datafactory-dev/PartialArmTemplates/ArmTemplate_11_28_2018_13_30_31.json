{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "hasa-datafactory-dev"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/HasaDataLakeDev')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "AzureDataLakeStore",
				"typeProperties": {
					"dataLakeStoreUri": "https://hasadldev.azuredatalakestore.net/webhdfs/v1",
					"tenant": "3aa4a235-b6e2-48d5-9195-7fcf05b459b0",
					"subscriptionId": "5025a3e4-a4b9-4786-b358-390ea614614b",
					"resourceGroupName": "hasarg"
				}
			}
		}
	]
}